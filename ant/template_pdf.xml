<project name="@PROJECT.NAME@_pdf" default="all" basedir="..">
    <import file="${basedir}${file.separator}conductor.xml"/>
	<target name="all" depends="@DELIVERABLE.NAME@2pdf">
	</target>

	<target name="@DELIVERABLE.NAME@2pdf" depends="use-init">
      <antcall target="dita2pdf">
        <param name="transtype" value="dita2pdf"/>
